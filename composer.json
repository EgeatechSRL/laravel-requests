{
    "name": "egeatech/laravel-requests",
    "description": "A simple package to better handle application requests.",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Guidolin",
            "email": "m.guidolin@egeatech.com",
            "homepage": "https://www.egeatech.com"
        }
    ],
    "homepage": "https://github.com/EgeatechSRL/laravel-requests",
    "keywords": ["Laravel", "Requests"],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "EgeaTech\\LaravelRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EgeaTech\\LaravelRequests\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "EgeaTech\\LaravelRequests\\Providers\\LaravelRequestsServiceProvider"
            ],
            "aliases": {
                "LaravelRequests": "EgeaTech\\LaravelRequests\\Facades\\LaravelRequests"
            }
        }
    }
}
